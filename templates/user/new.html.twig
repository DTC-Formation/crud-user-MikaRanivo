{% extends 'base.html.twig' %}

{% block title %}New User{% endblock %}

{% block body %}
<div class="container">
    <h1>Create new User</h1>

    {{ form_start(form) }}

   {{ form_row(form.Name,{"attr":{"placeholder": "Entrer nom"},"label":"Nom complet"}) }}
   {{ form_row(form.Age) }} 
   {{ form_row(form.Adress) }}
   {{ form_row(form.height) }}
   {{ form_row(form.CIN) }}
   {{ form_row(form.Tel) }}
  

    <br>
    <button  class="btn btn-primary" href="{{ path('app_user_index') }}" >{{ button_label|default('Save') }}</button>

    {{ form_end(form,{render_rest:true} )}}
    <br>
</div>
{% endblock %}
